{% extends "__layout.twig" %}
{% block content %}

<p>Элементов в type_flowers: {{ type_flowers|length }}</p>

<div class="container">
    <div class="row">
        <ul style="display: flex; flex-wrap: wrap; gap: 24px; list-style: none; padding-left: 0; margin: 0;">
            {% for item in type_flowers %}
              {% include '_menu_item.twig' with { item: item } %}
            {% endfor %}
          </ul>
    </div>
  </div>
  
{% endblock %}